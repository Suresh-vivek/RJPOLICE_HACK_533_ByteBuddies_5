{
  "manifest_version": 3,
  "name": "SafeNet",
  "version": "1.0",
  "description": "Check the safety status of a website using Google Safe Browsing API",
  "permissions": ["activeTab", "storage"],

  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
